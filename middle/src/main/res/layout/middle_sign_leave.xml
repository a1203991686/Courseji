<?xml version="1.0" encoding="utf-8"?><!-- 5已请假的签到 -->
<androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/middle_details_Leave"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/middle_details_guideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.32" />

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/middle_details_LeaveTitle"
        android:text="已请假"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/middle_details_guideline1"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintVertical_bias="0.06"
        android:textSize="30sp"
        android:textColor="@color/leave" />
</androidx.constraintlayout.widget.ConstraintLayout>